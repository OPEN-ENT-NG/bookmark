{
    "name": "{{ bookmarkVersion }}",
    "config": {
        "main": "net.atos.entng.bookmark.Bookmark",
        "port": 8032,
        {% if csrfToken %}"csrf-token": true,{% endif %}
        "app-name": "Signets",
        "app-address": "/bookmark",
        "app-icon": "bookmark-large",
        "app-type" : "WIDGET",
        "host": "https://{{ host }}",
        "ssl": true,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode": "HTTP",
        "app-registry.port": 8012,
        "mode": "prod",
        "entcore.port": 8009
    }
}